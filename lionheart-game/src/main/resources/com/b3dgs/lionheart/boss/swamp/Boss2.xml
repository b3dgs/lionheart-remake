<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<lionengine:featurable xmlns:lionengine="http://lionengine.b3dgs.com/xsd/9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://lionengine.b3dgs.com/xsd/9 http://lionengine.b3dgs.com/xsd/9/featurable.xsd">
    <lionengine:class>com.b3dgs.lionengine.helper.EntityHelper</lionengine:class>
    <lionengine:setup>com.b3dgs.lionheart.object.SetupEntity</lionengine:setup>
    <lionengine:features>
        <lionengine:feature>com.b3dgs.lionheart.object.EntityModel</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Stats</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Glue</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.BossSwampEffect</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.BossSwamp2</lionengine:feature>
    </lionengine:features>
    <lionengine:surface image="Boss2.png"/>
    <lionengine:frames horizontal="2" vertical="3"/>
    <lionengine:size width="248" height="120"/>
    <lionengine:origin>CENTER_BOTTOM</lionengine:origin>
    <lionengine:animations>
        <lionengine:animation name="idle" start="1" end="1" speed="1.0" reversed="false" repeat="false"/>
        <lionengine:animation name="land" start="2" end="2" speed="1.0" reversed="false" repeat="false"/>
    </lionengine:animations>
    <lionengine:collisions>
        <lionengine:collision name="ground" offsetX="-2" offsetY="88" width="20" height="1" mirror="false"/>
    </lionengine:collisions>
    <lionengine:layerable layerRefresh="7" layerDisplay="0"/>
    <lionengine:launcher level="0">
        <lionengine:launchable media="boss/swamp/Egg.xml" ox="86" oy="72" delay="0">
            <lionengine:force velocity="0" sensibility="0" vx="0.0" vy="0.0"/>
        </lionengine:launchable>
        <lionengine:launchable media="boss/swamp/Egg.xml" ox="86" oy="72" delay="25">
            <lionengine:force velocity="0" sensibility="0" vx="0.0" vy="0.0"/>
        </lionengine:launchable>
        <lionengine:launchable media="boss/swamp/Egg.xml" ox="86" oy="72" delay="50">
            <lionengine:force velocity="0" sensibility="0" vx="0.0" vy="0.0"/>
        </lionengine:launchable>
        <lionengine:launchable media="boss/swamp/Egg.xml" ox="86" oy="72" delay="75">
            <lionengine:force velocity="0" sensibility="0" vx="0.0" vy="0.0"/>
        </lionengine:launchable>
    </lionengine:launcher>
    <lionengine:launcher level="1">
        <lionengine:launchable media="boss/swamp/Bird.xml" ox="-64" oy="48" delay="10">
            <lionengine:force velocity="1.0" sensibility="0.0" vx="1.0" vy="1.0"/>
        </lionengine:launchable>
        <lionengine:launchable media="boss/swamp/Bird.xml" ox="-64" oy="48" delay="160">
            <lionengine:force velocity="1.0" sensibility="0.0" vx="1.0" vy="1.0"/>
        </lionengine:launchable>
    </lionengine:launcher>
    <lionengine:collidable group="2" accepted="1"/>
    <stats health="6"/>
    <shade horizontal="1" vertical="1"/>
    <alwaysUpdate>true</alwaysUpdate>
</lionengine:featurable>