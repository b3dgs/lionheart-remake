<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="Lionheart Remake Editor">
         <property
               name="appName"
               value="Lionheart Remake Editor">
         </property>
         <property
               name="lifeCycleURI"
               value="bundleclass://com.b3dgs.lionengine.editor/com.b3dgs.lionengine.editor.Manager">
         </property>
         <property
               name="applicationXMI"
               value="platform:/plugin/com.b3dgs.lionengine.editor/Application.e4xmi">
         </property>
      </product>
   </extension>
   <extension
         id="com.b3dgs.lionheart.editor.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="always"
            uri="fragment.e4xmi">
      </fragment>
      <processor
            apply="always"
            beforefragment="false"
            class="com.b3dgs.lionheart.editor.ApplicationConfiguration">
      </processor>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.b3dgs.lionheart.editor.object.properties.geyzer.GeyzerDefinedTester"
            id="com.b3dgs.lionheart.editor.geyzerDefinedTester"
            namespace="com.b3dgs.lionheart.editor.geyzerDefinedTester"
            properties="enableGeyzer,disableGeyzer"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionheart.editor.object.properties.hotfireball.HotFireBallDefinedTester"
            id="com.b3dgs.lionheart.editor.hotFireBallDefinedTester"
            namespace="com.b3dgs.lionheart.editor.hotFireBallDefinedTester"
            properties="enableHotFireBall,disableHotFireBall"
            type="java.lang.Object">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.b3dgs.lionheart.editor.properties.enableGeyzer">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.geyzerDefinedTester.enableGeyzer">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.properties.disableGeyzer">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.geyzerDefinedTester.disableGeyzer">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.properties.disableHotFireBall">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.hotFireBallDefinedTester.disableHotFireBall">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.properties.enableHotFireBall">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.hotFireBallDefinedTester.enableHotFireBall">
         </test>
      </definition>
   </extension>
</plugin>
