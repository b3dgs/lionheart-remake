<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<lionengine:featurable xmlns:lionengine="http://lionengine.b3dgs.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://lionengine.b3dgs.com/xsd/9 http://lionengine.b3dgs.com/xsd/9/featurable.xsd">
    <lionengine:class>com.b3dgs.lionengine.helper.EntityHelper</lionengine:class>
    <lionengine:features>
        <lionengine:feature>com.b3dgs.lionheart.object.EntityModel</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Effect</lionengine:feature>
    </lionengine:features>
    <lionengine:surface image="ExplodeBig.png"/>
    <lionengine:frames horizontal="5" vertical="4"/>
    <lionengine:origin>MIDDLE</lionengine:origin>
    <sfx_explode count="5"/>
    <lionengine:animations>
        <lionengine:animation end="20" name="idle" repeat="false" reversed="false" speed="0.48" start="1"/>
    </lionengine:animations>
    <lionengine:rasterable extern="true"/>
    <lionengine:layerable layerDisplay="7" layerRefresh="1"/>
    <alwaysUpdate>true</alwaysUpdate>
</lionengine:featurable>
