<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<lionengine:featurable xmlns:lionengine="http://lionengine.b3dgs.com/xsd/9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://lionengine.b3dgs.com/xsd/9 http://lionengine.b3dgs.com/xsd/9/featurable.xsd">
    <lionengine:class>com.b3dgs.lionengine.helper.EntityHelper</lionengine:class>
    <lionengine:setup>com.b3dgs.lionheart.object.SetupEntity</lionengine:setup>
    <lionengine:features>
        <lionengine:feature>com.b3dgs.lionheart.object.EntityModel</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Stats</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Patrols</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Patrol</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Hurtable</lionengine:feature>
    </lionengine:features>
    <lionengine:surface image="surface/Bee.png"/>
    <lionengine:frames horizontal="2" vertical="1"/>
    <lionengine:size width="32" height="32"/>
    <lionengine:origin>CENTER_BOTTOM</lionengine:origin>
    <lionengine:animations>
        <lionengine:animation name="idle" start="1" end="1" speed="0.25" reversed="false" repeat="false"/>
        <lionengine:animation name="patrol" start="1" end="2" speed="0.25" reversed="false" repeat="true"/>
        <lionengine:animation name="turn" start="1" end="1" speed="1.0" reversed="false" repeat="false"/>
        <lionengine:animation name="fall" start="1" end="1" speed="0.25" reversed="false" repeat="false"/>
        <lionengine:animation name="hurt" start="1" end="2" speed="1.0" reversed="false" repeat="false"/>
    </lionengine:animations>
    <lionengine:collisions>
        <lionengine:collision name="attack" offsetX="0" offsetY="2" width="28" height="28" mirror="false"/>
        <lionengine:collision name="body" offsetX="0" offsetY="2" width="28" height="28" mirror="false"/>
    </lionengine:collisions>
    <lionengine:categories>
        <lionengine:category name="center" axis="Y" x="0" y="0">
            <lionengine:group>ground</lionengine:group>
            <lionengine:group>slope_0_left</lionengine:group>
            <lionengine:group>slope_1_left</lionengine:group>
            <lionengine:group>slope_0_right</lionengine:group>
            <lionengine:group>slope_1_right</lionengine:group>
            <lionengine:group>steep_0_left</lionengine:group>
            <lionengine:group>steep_left_ground</lionengine:group>
            <lionengine:group>steep_1_left</lionengine:group>
            <lionengine:group>steep_0_right</lionengine:group>
            <lionengine:group>steep_1_right</lionengine:group>
            <lionengine:group>steep_1_right_spike</lionengine:group>
        </lionengine:category>
    </lionengine:categories>
    <lionengine:state>com.b3dgs.lionheart.object.state.StateIdle</lionengine:state>
    <lionengine:rasterable extern="true"/>
    <lionengine:layerable layerRefresh="3" layerDisplay="6"/>
    <lionengine:collidable group="2" accepted="1"/>
    <stats health="1"/>
    <hurtable fall="true" effect="effect/swamp/ExplodeBig.xml"/>
    <shade horizontal="2" vertical="1"/>
    <patrol amplitude="48" mirror="false" sv="0.7"/>
</lionengine:featurable>