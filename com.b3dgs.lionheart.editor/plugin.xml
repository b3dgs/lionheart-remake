<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="Lionheart Remake Editor">
         <property
               name="appName"
               value="Lionheart Remake Editor">
         </property>
         <property
               name="lifeCycleURI"
               value="bundleclass://com.b3dgs.lionengine.editor/com.b3dgs.lionengine.editor.Manager">
         </property>
         <property
               name="applicationXMI"
               value="platform:/plugin/com.b3dgs.lionengine.editor/Application.e4xmi">
         </property>
      </product>
   </extension>
   <extension
         id="com.b3dgs.lionheart.editor.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="always"
            uri="fragment.e4xmi">
      </fragment>
      <processor
            apply="always"
            beforefragment="false"
            class="com.b3dgs.lionheart.editor.ApplicationConfiguration">
      </processor>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.b3dgs.lionheart.editor.object.properties.geyzer.GeyzerPropertyTester"
            id="com.b3dgs.lionheart.editor.part.properties.geyzer.tester"
            namespace="com.b3dgs.lionheart.editor.part.properties.geyzer.tester"
            properties="enable,disable"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionheart.editor.object.properties.hotfireball.HotFireBallPropertyTester"
            id="com.b3dgs.lionheart.editor.part.properties.hotfireball.tester"
            namespace="com.b3dgs.lionheart.editor.part.properties.hotfireball.tester"
            properties="enable,disable"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionheart.editor.object.properties.spike.SpikePropertyTester"
            id="com.b3dgs.lionheart.editor.part.properties.spike.tester"
            namespace="com.b3dgs.lionheart.editor.part.properties.spike.tester"
            properties="enable,disable"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionheart.editor.object.properties.spider.SpiderPropertyTester"
            id="com.b3dgs.lionheart.editor.part.properties.spider.tester"
            namespace="com.b3dgs.lionheart.editor.part.properties.spider.tester"
            properties="enable,disable"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionheart.editor.object.properties.shooter.ShooterPropertyTester"
            id="com.b3dgs.lionheart.editor.part.properties.shooter.tester"
            namespace="com.b3dgs.lionheart.editor.part.properties.shooter.tester"
            properties="enable,disable"
            type="java.lang.Object">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.geyzer.enable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.geyzer.tester.enable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.geyzer.disable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.geyzer.tester.disable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.hotfireball.disable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.hotfireball.tester.disable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.hotfireball.enable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.hotfireball.tester.enable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.spike.enable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.spike.tester.enable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.spike.disable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.spike.tester.disable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.spider.disable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.spider.tester.disable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.spider.enable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.spider.tester.enable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.shooter.enable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.shooter.tester.enable">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionheart.editor.part.properties.shooter.disable">
         <test
               forcePluginActivation="true"
               property="com.b3dgs.lionheart.editor.part.properties.shooter.tester.disable">
         </test>
      </definition>
   </extension>
</plugin>
